//This program is used for search the longest child-array in a array. The time complexity is O(n^2)
#include "iostream"
using namespace std;

int main()
{
    int x;
    while(cin>>x && x!=0)
    {
       int n;
       cin>>n;
       int a[1000];
       for(int i=0;i<n;i++)
           cin>>a[i];
    int b[1000]={0};
    int b[1]=0;
    for (int i =1; i < n; ++i)
    {
        for(int j=i-1;j>=1;j--)
        {
            if(a[i]>a[j])
                {
                    b[i]=b[j]+1;
                    break;
                }
        }
    }
}
    int max=b[0];
    for(i=1;i<x;i++)
    {
        if(b[i]>max)
            max=b[i];
    }
    cout<<max<<endl;
    return 0;
}
