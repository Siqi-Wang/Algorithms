//This program is used for search the longest child-array in a array. The time complexity is O(n^2)
#include "iostream"
using namespace std;

int main()
{
    int n;
	cout<<"Please input the length of the array:";
    while(cin>>n && n!=0)
    {
	   int a[1000];
	   int i;
	   cout<<"Please input the array:";
       for(i=0;i<n;i++)
           cin>>a[i];
    int b[1000]={1};
    for (i=1; i<n; ++i)
    {
        for(int j=0;j<i ;j++)
        {
            if(a[j]<a[i] && b[j]+1>b[i])
				b[i]=b[j]+1;
        }
    }
    int max=b[0];
    for(i=1;i<n;i++)
    {
        if(b[i]>max)
            max=b[i];
    }
	cout<<"The longest length of child array is:";
    cout<<max<<endl;
    return 0;
	}
}
